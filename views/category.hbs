<!--================Category Product Area =================-->
<section class="cat_product_area section_padding border_top">
    <div class="container">
        <div class="row">
            <div class="col-lg-3">
                <div class="left_sidebar_area">
                    <aside class="left_widgets p_filter_widgets sidebar_box_shadow">
                        <div class="l_w_title">
                            <h3>Danh mục</h3>
                        </div>
                        <div class="widgets_inner">
                            <ul class="list">
                                <li>
                                    <a href="/category?q=thoi_trang_nam">Thời trang nam</a>
                                </li>
                                <li>
                                    <a href="/category?q=thoi_trang_nu">Thời trang nữ</a>

                                </li>
                                <li>
                                    <a href="/category?q=giay">Giày</a>

                                </li>
                                <li class="sub-menu">
                                    <a href="#Electric" class=" d-flex justify-content-between">
                                        Thiết bị điện tử
                                        <div class="right ti-plus"></div>
                                    </a>
                                    <ul>
                                        <li>
                                            <a href="/category?q=thiet_bi_gia_dung">Thiết bị gia dụng</a>
                                        </li>
                                        <li>
                                            <a href="/category?q=dien_thoai_thong_minh">Điện thoại thông minh</a>
                                        </li>
                                        <li>
                                            <a href="/category?q=dung_cu_nha_bep">Dụng cụ nhà bếp</a>
                                        </li>
                                        <li>
                                            <a href="/category?q=phu_kien_may_tinh">Phụ kiện máy tính</a>
                                        </li>
                                    </ul>
                                </li>

                                <li>
                                    <a href="/category?q=do_uong">Đồ uống</a>
                                </li>
                                <li>
                                    <a href="/category?q=thuc_pham">Thực phẩm</a>
                                </li>
                            </ul>
                        </div>
                    </aside>
                    <aside class="left_widgets p_filter_widgets sidebar_box_shadow">
                        <div class="l_w_title">
                            <h3>Bộ lọc</h3>
                        </div>
                        <div class="widgets_inner">
                            <ul class="list">
                                <p>Thương hiệu</p>
                                <li>
                                    <input type="radio" aria-label="Radio button for following text input">
                                    <a href="#">Apple</a>
                                </li>
                                <li>
                                    <input type="radio" aria-label="Radio button for following text input">
                                    <a href="#">Asus</a>
                                </li>
                                <li class="active">
                                    <input type="radio" aria-label="Radio button for following text input">
                                    <a href="#">Gionee</a>
                                </li>
                                <li>
                                    <input type="radio" aria-label="Radio button for following text input">
                                    <a href="#">Micromax</a>
                                </li>
                                <li>
                                    <input type="radio" aria-label="Radio button for following text input">
                                    <a href="#">Samsung</a>
                                </li>
                            </ul>
                            <ul class="list">
                                <p>Màu sắc</p>
                                <li>
                                    <input type="radio" aria-label="Radio button for following text input">
                                    <a href="#">Đen</a>
                                </li>
                                <li>
                                    <input type="radio" aria-label="Radio button for following text input">
                                    <a href="#">Đen nhạt</a>
                                </li>
                                <li>
                                    <input type="radio" aria-label="Radio button for following text input">
                                    <a href="#">Đen với đỏ</a>
                                </li>
                                <li>
                                    <input type="radio" aria-label="Radio button for following text input">
                                    <a href="#">Vàng</a>
                                </li>
                                <li>
                                    <input type="radio" aria-label="Radio button for following text input">
                                    <a href="#">Spacegrey</a>
                                </li>
                            </ul>
                        </div>
                    </aside>

                    <aside class="left_widgets p_filter_widgets price_rangs_aside sidebar_box_shadow">
                        <div class="l_w_title">
                            <h3>Bộ lọc giá</h3>
                        </div>
                        <div class="widgets_inner">
                            <div class="range_item">
                                <!-- <div id="slider-range"></div> -->
                                <input type="text" class="js-range-slider" value="" />
                                <div class="d-flex align-items-center">
                                    <div class="price_text">
                                        <p>Giá :</p>
                                    </div>
                                    <div class="price_value d-flex justify-content-center">
                                        <input type="text" class="js-input-from" id="amount" readonly />
                                        <span>to</span>
                                        <input type="text" class="js-input-to" id="amount" readonly />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </aside>
                </div>
            </div>
            <div class="col-lg-9">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="product_top_bar d-flex justify-content-between align-items-center">
                            <div class="single_product_menu product_bar_item">
                                <h2>Danh sách sản phẩm</h2>
                            </div>
                            <div class="product_top_bar_iner product_bar_item d-flex">
                                <div class="product_bar_single">
                                    <select class="wide">
                                        <option data-display="Sắp xếp">Sắp xếp</option>
                                        <option value="1">Some option</option>
                                        <option value="2">Another option</option>
                                        <option value="3">Potato</option>
                                    </select>
                                </div>
                                <div class="product_bar_single">
                                    <select>
                                        <option data-display="Hiện thị 12">Hiện thị 12</option>
                                        <option value="1">Hiện thị 20</option>
                                        <option value="2">Hiện thị 30</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    {{#each list}}
                    <div class="col-lg-4 col-sm-6">
                        <div class="single_category_product">
                            <div class="single_category_img">
                                <a href="/singleproduct?q={{this._id}}"> <img src="{{this.img}}" alt=""></a>

                                <div class="category_social_icon">
                                    <ul>
                                        <li><a href="#"><i class="ti-heart"></i></a></li>
                                        <li><a href="#"><i class="ti-bag"></i></a></li>
                                    </ul>
                                </div>
                                <div class="category_product_text">
                                    <a href="/singleproduct?q={{this._id}}">
                                        <h5>{{this.name}}</h5>
                                    </a>
                                    <p>{{this.price}} $</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                    <div class="col-lg-12 text-center">
                        <a href="#" class="btn_2">Xem thêm</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--================End Category Product Area =================-->

